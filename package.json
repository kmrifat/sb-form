{
  "name": "sb-form",
  "description": "SB Form is a dynamic form build with vue3 for laravel and vue application",
  "author": "K M Rifat ul alom <kmrifat@gmail.com>",
  "keywords": [
    "vue dynamic form",
    "laravel vue form"
  ],
  "private": false,
  "homepage": "https://github.com/kmrifat/sb-form",
  "version": "0.2.35",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "src",
    "dist"
  ],
  "release": {
    "branches": [
      "master",
      {
        "name": "preview",
        "prerelease": "preview",
        "mergeRange": true
      }
    ]
  },

  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.5",
    "bootstrap": "^5.2.0-beta1",
    "laravel-vue-pagination": "^3.0.0",
    "toastr": "^2.1.4",
    "vue": "^3.2.25",
    "vue-router": "^4.1.2"
  },
  "devDependencies": {
    "@aacassandra/vue3-progressbar": "^1.0.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vuepress/plugin-search": "^1.9.9",
    "axios": "^0.27.2",
    "sass": "^1.51.0",
    "semantic-release": "^24.2.2",
    "vite": "^2.9.9",
    "vue-select": "^4.0.0-beta.3",
    "vuepress": "^2.0.0-beta.61",
    "vuex": "^4.0.2",
    "vuex-persistedstate": "^4.1.0"
  }
}
